{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "scripts": {
    "wifi-start": "apicloud wifiStart --port 1118",
    "wifi-stop": "apicloud wifiStop --port 1118",
    "wifi-sync": "apicloud wifiSync --project ./dist --updateAll false --port 1118",
    "wifi-log": "apicloud wifiLog --port 1118",
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-modern": "vue-cli-service build --modern",
    "webpack-config": "vue-cli-service inspect > webpack.js"
  },
  "author": "{{ author }}",
  "license": "MIT",
  "keywords": [    
    "apicloud",
    "vue",
    "vue2",
    "vant",
    "webpack",
    "app",
    "hybird",
    "ios",
    "android",
    "javascript",
    "nodejs",
    "vue3-cli"
  ],
  
  "dependencies": {
    "core-js": "^3.3.2",
    "vant": "^2.2.10",
    "vue": "^2.6.10",
    "vue-svg-icon": "^1.2.9",
    "vux": "^2.9.4"
  },
  "devDependencies": {
    "@vue/cli": "^4.0.5",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "apicloud-cli": "^0.2.0",
    "babel-plugin-import": "^1.12.2",
    "klaw-sync": "^6.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  }
